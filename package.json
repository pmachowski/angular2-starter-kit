{
  "name": "angular2-starter-kit",
  "version": "0.1.2",
  "description": "Save yourself countless hours on perfecting your stack â€“ it's already done.",
  "author": "Peter Machowski <peter@machowski.co> (https://github.com/pmachowski)",
  "license": "MIT",
  "homepage": "http://www.angular2starterkit.com",
  "scripts": {
    "postinstall": "npm run typings",
    "reinstall": "npm run clean:dist && rimraf node_modules && npm install",
    "clean": "rimraf dist logs",
    "clean:dist": "rimraf dist",
    "clean:logs": "rimraf logs",
    "prebuild": "npm run clean:dist",
    "build:dev": "node tools/build.dev.js",
    "build": "node tools/build.prod.js",
    "prestart": "npm run build",
    "start": "npm run serve",
    "serve": "node ./dist/private/serve.js",
    "serve:pm2": "pm2 start ecosystem.json",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start --single-run --path--",
    "unit:watch": "karma start --path--",
    "e2e": "protractor protractor.conf.js",
    "lint": "node tools/lint.js",
    "lint:watch": "node tools/lint.js --watch",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "status": "pm2 show $(node -e \"console.log(require('./ecosystem.json').name)\")",
    "logs": "pm2 logs $(node -e \"console.log(require('./ecosystem.json').name)\")",
    "pm2": "pm2",
    "webpack": "webpack",
    "typings": "typings install",
    "karma": "karma",
    "protractor": "protractor",
    "webdriver": "webdriver-manager"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "angular2-universal": "0.94.2",
    "preboot": "2.0.6",
    "compression": "^1.6.1",
    "css": "^2.2.1",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "express": "^4.13.3",
    "parse5": "^1.5.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "serve-static": "^1.10.2",
    "zone.js": "0.6.11"
  },
  "devDependencies": {
    "bootstrap": "^3.3.6",
    "chalk": "^1.1.1",
    "chokidar": "^1.4.2",
    "compression-webpack-plugin": "^0.3.0",
    "css-loader": "^0.23.1",
    "copy-webpack-plugin": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.6",
    "karma-mocha-reporter": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "memory-fs": "^0.3.0",
    "phantomjs-prebuilt": "^2.1.4",
    "pm2": "^1.0.2",
    "postcss-cssnext": "^2.4.0",
    "postcss-loader": "^0.8.0",
    "protractor": "^3.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.0",
    "ts-loader": "^0.8.1",
    "tslint": "^3.3.0",
    "typescript": "^1.8.7",
    "typings": "0.7.12",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "0.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:pmachowski/angular2-starter-kit.git"
